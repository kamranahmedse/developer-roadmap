---
import BaseLayout, { type Props as BaseLayoutProps } from './BaseLayout.astro';

export interface Props extends BaseLayoutProps {}

const props = Astro.props;
---

<BaseLayout {...props} noIndex={true}>
  <div slot='ad-slot-script'></div>
  <div slot='ad-slot'></div>

  <slot />
  <div slot='login-popup'></div>
  <div slot='page-footer'></div>
</BaseLayout>
